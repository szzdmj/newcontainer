{
  "name": "shenzhou-app-to-cloudflare", // ✅ 必须与你部署时使用的 Worker 名一致
  "compatibility_date": "2024-05-01",
  "compatibility_flags": ["nodejs_compat"],
  "node_compat": true,
  "durable_objects": {
    "bindings": [
      {
        "name": "MY_CONTAINER",
        "class_name": "MyContainer"
      }
    ]
  },
  "containers": [
    {
      "binding": "MY_CONTAINER",
      "image": "ghcr.io/szzdmj/shenzhou-app:latest" // ✅ 你的公开镜像
    }
  ],
  "build": {
    "command": "echo done"
  }
}

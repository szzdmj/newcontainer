{
  "name": "shenzhou-app-to-cloudflare",
  "main": "src/worker.ts", // 👈 指定入口文件
  "compatibility_date": "2024-04-18",
  "containers": [
    {
      "name": "mycontainer",
      "image": "ghcr.io/szzdmj/shenzhou-app:latest",
      "class_name": "MyContainer"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "MY_CONTAINER",
        "class_name": "MyContainer"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["MyContainer"]
    }
  ]
}
